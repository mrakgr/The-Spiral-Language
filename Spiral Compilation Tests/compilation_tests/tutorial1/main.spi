union t x =
    | A :: forall x. x -> t x
    | B :: forall x y. x * y -> t (x * y)

// union t x =
//     | A : x
//     | B : x

let f forall y. (x : t y) : y =
    match x with
    | A a =>
        inl a = a
        a
    | B a =>
        inl a = a
        a
