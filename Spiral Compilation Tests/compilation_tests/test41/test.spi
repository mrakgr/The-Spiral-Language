// Do the nested patterns work?
inl main() =
    match dyn Nil : _ (i32 * i32) with
    | Nil => "No elements", 0
    | Cons: (q,w),Nil => "One element.", q+w
    | Cons: (q,w),(Cons:(e,r),Nil) => "Two elements.",q+w+e+r
    | Cons: (q,w),(Cons:(e,r),(Cons:(t,y),l)) => "At least three elements.",q+w+e+r+t+y